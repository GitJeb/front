
    <div class="container">
        <div class="row">

            {{#each uploads as | upload |}}
                <div class="col-md-3">
                    <div class="thumbnail">
                        <!-- pic display -->
                        <div class="pic-display">
                              {{#fileType upload}} {{/fileType}}
                              <div class='info-overlay'>
                                {{upload.title}} by: {{upload._owner.email}}
                              </div>
                        </div>
                    </div>
                </div>
            {{/each}}

          </div>


        </div>
      </div>
